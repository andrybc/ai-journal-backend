{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "files": {
    "ignore": [
      "node_modules",
      ".git",
      ".env",
      "package-lock.json",
      "package.json",
      "data/**/*",
      "backend/swaggerhub/**/*"
    ]
  },
  "formatter": {
    "indentStyle": "space"
  },
